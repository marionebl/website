{
  "name": "stacked-website",
  "version": "1.0.0",
  "description": "The website for Stacked",
  "main": "index.js",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "http-server": "^0.10.0",
    "less": "^2.7.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "nodemon": "^1.12.1"
  },
  "scripts": {
    "watch:node-less": "nodemon -e less -x \"npm run node-less\"",
    "node-less": "lessc --strict-math=on --clean-css='advanced' --autoprefix='last 2 versions, not ie < 11, last 2 ChromeAndroid versions, last 2 Android versions, last 1 ExplorerMobile versions' assets/css/style.less > assets/css/style.css",
    "watch": "npm run watch:node-less",
    "serve": "http-server .",
    "start": "concurrently \"npm run watch\" \"npm run serve\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stackedapp/stacked-website.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stackedapp/stacked-website/issues"
  },
  "homepage": "https://github.com/stackedapp/stacked-website#readme"
}
